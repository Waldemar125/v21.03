from turtle import *
screensize(1000, 1000)
tracer(1)
n = 10
x = 10
left(90)
down()
for i in range(2):
    forward(3 * x * n)
    right(90)
forward(x * 10)
right(90)
forward(2 * x * n)
left(90)
forward(2 * x * n)
right(90)
forward(x * n)
tracer(100)
up()
for x in range(-100, 100):
    for y in range(-100, 100):
        goto(x * n, y * n)
        dot(5, 'red')
done()
