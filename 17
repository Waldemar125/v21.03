s = [int(i) for i in open('17-370').readlines()]
mn = min([i for i in s if len(str(abs(i))) == 3 and abs(i) % 10 == 3])
k, m = 0, 10 ** 15
for i in range(len(s) - 1):
    a, b = s[i], s[i + 1]
    if [len(str(abs(j))) == 4 for j in (a, b)].count(True) == 1:
        if (a ** 2 + b ** 2) % mn == 0:
            k += 1
            m = min(m, a ** 2 + b ** 2)
print(k, m)
